{
  "version": 2,
  "name": "oslopride-website",
  "builds": [
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/a/(?<slug>[^/]+)$", "dest": "/article?slug=$slug" },
    { "src": "/events/(?<id>[^/]+)$", "dest": "/event?id=$id" },
    { "src": "/p/(?<slug>[^/]+)$", "dest": "/page?slug=$slug" }
  ],
  "public": true
}
